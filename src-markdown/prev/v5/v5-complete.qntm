// Technical Writing Templates and Standards
@framework{technical_writing}:p{
  description:"Comprehensive system for creating effective technical documentation",
  principles:["educational","multi-perspective","practical","visual","progressive"],
  structure:["introduction","concepts","implementation","advanced"]
}

@atlas->@framework{technical_writing}:p{rel:"employs",purpose:"knowledge_transfer"}

@tmpl{technical_document}:p{
  sections:[
    "title:concise descriptive name of component or process",
    "overview:one-paragraph summary of purpose and role",
    "architecture:key components, relationships, and boundaries",
    "implementation:practical guide to structure and function",
    "examples:concrete implementations showing practical application",
    "constraints:limitations, edge cases, and failure modes",
    "references:related components, external resources"
  ],
  style:"clear, concise language with progressive disclosure",
  includes:"diagrams, code samples, and contextual examples"
}

@tmpl{api_documentation}:p{
  sections:[
    "endpoint:complete URL path and method",
    "parameters:name, type, constraints, default values",
    "authentication:required permissions and methods",
    "response:structure, status codes, and examples",
    "errors:possible error codes with troubleshooting",
    "rate_limits:usage constraints and quotas",
    "examples:complete request/response pairs"
  ],
  style:"precise, complete specifications with edge cases"
}

@tmpl{process_documentation}:p{
  sections:[
    "purpose:clear objective and value proposition",
    "actors:roles and responsibilities of participants",
    "prerequisites:required resources, permissions, states",
    "sequence:step-by-step progression with decision points",
    "outputs:expected results and success criteria",
    "exceptions:alternative paths and error handling",
    "diagrams:visual representation of workflow"
  ],
  style:"task-oriented with clear user perspective"
}

@framework{technical_writing}->@tmpl{technical_document}:p{rel:"incorporates",usage:"system_components"}
@framework{technical_writing}->@tmpl{api_documentation}:p{rel:"incorporates",usage:"interfaces"}
@framework{technical_writing}->@tmpl{process_documentation}:p{rel:"incorporates",usage:"workflows"}

// Commit Message Templates and Styles
@framework{commit_style}:p{
  description:"Standardized format for version control commit messages",
  purpose:"clear communication of change intent and scope",
  format:"action: brief description",
  constraints:"present tense, 50 char max subject line, no period"
}

@atlas->@framework{commit_style}:p{rel:"employs",purpose:"change_management"}

@tmpl{commit_message}:p{
  subject_format:"action: concise description of change",
  actions:[
    "add:introducing new features/files",
    "update:enhancing existing functionality",
    "fix:correcting bugs or issues",
    "refactor:restructuring without behavior change",
    "style:formatting, whitespace, naming",
    "test:adding/modifying tests",
    "docs:documentation only changes",
    "chore:maintenance tasks, dependencies",
    "integrate:merging branches, integrating systems"
  ],
  body_format:"bullet points with additional context",
  examples:[
    "add: user authentication module",
    "fix: prevent duplicate transaction processing",
    "docs: update API reference for payment endpoints"
  ]
}

@pol{attribution}:p{
  description:"Guidelines for proper attribution of work and contributions",
  principle:"accurate representation of authorship and sources",
  human_authorship:"all outputs represent authentic human work product",
  no_ai_disclosure:"never attribute work to AI systems in any context",
  implementation:"commit messages, documentation, and metadata should only attribute human authors"
}

@atlas->@pol{attribution}:p{rel:"strictly_follows"}

