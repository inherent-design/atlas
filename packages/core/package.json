{
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@duckdb/node-api": "1.4.3-r.3",
    "@huggingface/transformers": "^3.8.1",
    "@langchain/textsplitters": "^1.0.1",
    "@lingui/core": "^5.7.0",
    "@qdrant/js-client-rest": "^1.16.2",
    "chokidar": "^5.0.0",
    "duckdb": "^1.4.3",
    "fast-glob": "^3.3.3",
    "ioredis": "^5.9.1",
    "kysely": "^0.28.9",
    "meilisearch": "^0.55.0",
    "p-limit": "^7.2.0",
    "p-map": "^7.0.4",
    "p-retry": "^7.1.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "voyageai": "^0.1.0",
    "webgpu": "^0.3.8"
  },
  "description": "Core library for Atlas semantic memory - embeddings, storage, search",
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/pg": "^8.16.0",
    "@types/picomatch": "^4.0.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@webgpu/types": "^0.1.68",
    "esbuild": "^0.24.2",
    "happy-dom": "^20.0.11",
    "picomatch": "^4.0.3",
    "prettier": "^3.7.4",
    "tsdown": "0.20.0-beta.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24.12.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./domain/*": {
      "types": "./dist/domain/*/index.d.mts",
      "default": "./dist/domain/*/index.mjs"
    },
    "./services/*": {
      "types": "./dist/services/*/index.d.mts",
      "default": "./dist/services/*/index.mjs"
    },
    "./shared/*": {
      "types": "./dist/shared/*.d.mts",
      "default": "./dist/shared/*.mjs"
    }
  },
  "files": [
    "dist",
    "src",
    "!**/*.test.ts"
  ],
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "name": "@inherent.design/atlas-core",
  "peerDependencies": {
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "prepublishOnly": "pnpm clean && pnpm build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "version": "0.0.0-alpha.7"
}
