{
  "dependencies": {
    "@inherent.design/atlas-core": "workspace:*"
  },
  "description": "Claude Code plugin for Atlas semantic memory",
  "engines": {
    "node": ">=24.12.0"
  },
  "files": [
    ".claude-plugin",
    "hooks",
    "dist/scripts"
  ],
  "module": "index.ts",
  "name": "@inherent.design/atlas-claude-code",
  "peerDependencies": {
    "typescript": "^5"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/scripts/*.js",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "type": "module",
  "version": "0.0.0-alpha.7",
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
