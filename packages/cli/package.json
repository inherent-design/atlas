{
  "bin": {
    "atlas": "./dist/index.js"
  },
  "dependencies": {
    "@inherent.design/atlas-core": "workspace:*",
    "@qdrant/js-client-rest": "^1.16.2",
    "commander": "^14.0.2",
    "duckdb": "^1.4.3",
    "ioredis": "^5.9.1",
    "kysely": "^0.28.9",
    "meilisearch": "^0.55.0",
    "pg": "^8.16.3"
  },
  "description": "CLI for Atlas semantic memory - ingest, search, and manage context",
  "devDependencies": {
    "@types/node": "^25.0.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=24.12.0"
  },
  "files": [
    "dist",
    "!dist/**/*.test.js"
  ],
  "module": "src/index.ts",
  "name": "@inherent.design/atlas-cli",
